    "storybook": "^7.0.18",
    "tailwindcss": "3.3.1",
    "ts-jest": "^29.1.0",
    "tsconfig-paths-webpack-plugin": "^4.0.1",
    "typescript": "5.0.4"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "msw": {
    "workerDirectory": "public"
  }
}